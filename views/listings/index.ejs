<% layout('/layouts/boilerplate') -%>


<body>
    <h3>All Listings</h3>
 
    <form method="get" action="listings/new">
        <button>Create Post</button>
    </form>  

    <ul>   
        <% for (let listing of allListings) { %>
           <li>
                <a href="/listings/<%= listing._id %>"><%= listing.name %></a>
                </li>  
        <% } %>
    </ul>  
</body>
